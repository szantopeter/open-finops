{
  "name": "open-finops",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --watch=false --no-progress --code-coverage --browsers ChromeHeadlessCI",
    "prod": "ng build",
    "compile-all": "npx tsc -p tsconfig.json --noEmit && npx tsc -p tsconfig.spec.json --noEmit && npx ng build --configuration production",
    "lint": "ng lint",
    "lint:staged": "lint-staged",
    "prepare": "husky",
    "generate": "./generate-types-from-schema.sh"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^20.3.12",
    "@angular/compiler": "^20.3.12",
    "@angular/core": "^20.3.12",
    "@angular/forms": "^20.3.12",
    "@angular/platform-browser": "^20.3.12",
    "@angular/platform-browser-dynamic": "^20.3.12",
    "@angular/router": "^20.3.12",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/papaparse": "^5.5.1",
    "ag-grid-angular": "^35.0.0",
    "ag-grid-community": "^35.0.0",
    "animate.css": "^4.1.1",
    "echarts": "^5.6.0",
    "idb-keyval": "^6.2.2",
    "launchdarkly-js-client-sdk": "~3.8.1",
    "ngx-echarts": "^20.0.2",
    "papaparse": "^5.5.3",
    "rxjs": "~7.8.2",
    "tippy.js": "^6.3.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/build": "^20.3.10",
    "@angular/cli": "~20.3.0",
    "@angular/compiler-cli": "^20.3.12",
    "@eslint/js": "^9.39.2",
    "@types/jasmine": "~5.1.9",
    "husky": "~9.1.7",
    "jasmine-core": "^5.9.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-junit-reporter": "2.0.1",
    "karma-spec-reporter": "0.0.36",
    "lint-staged": "^16.2.6",
    "madge": "^8.0.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.53.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npx --no-install prettier --write",
      "eslint -c eslint.local.config.cjs --cache --cache-location .eslintcache --fix"
    ]
  },
  "resolutions": {
    "chokidar": "3.6.0",
    "readdirp": "3.6.0",
    "picomatch": "2.3.1",
    "glob-parent": "5.1.2"
  }
}
